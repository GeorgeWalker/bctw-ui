{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
     {
      "type": "chrome",
      "request": "launch",
      "name": "debug vue",
      "url": "http://localhost:1111",
      "webRoot": "${workspaceFolder}",
      // "webRoot": "${workspaceFolder}/frontend/src",
      "breakOnLoad": true,
       "sourceMapPathOverrides": {
        "../*": "${webRoot}/vue/frontend/src/*"
      },
      "trace": true,
      "sourceMaps": true,
      // "runtimeExecutable": "/mnt/c/Program Files (x86)/Google/Chrome/Application/chrome.exe"
      // "pathMapping": {
      //   "__parcel_source_root": "${workspaceFolder}/dist"
      // },
      //  "env": {
      //   "PARCEL_WORKERS": "0" // Run workers in same thread for debugging
      // },
    },
    {
      "name": "launch keycloak server.js",
      "program": "${workspaceFolder}/backend/js/server.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "envFile": "${workspaceFolder}/backend/.env"
    }
  ]
}